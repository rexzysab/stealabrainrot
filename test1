  logsContainer=nil;local Active=true;local function parseGeneration(text) if ( not text or (text=="")) then return 0;end text=tostring(text):gsub(",",""):gsub("%$",""):   
  match("^%s*(.-)%s*$") or text ;if text:match("/[sS]") then text=text:gsub("/[sS]","");end local minPart,secPart=text:match("^(%d+)%s*[mM]%s+(%d+)%s*[sS]$");if  not       
  minPart then minPart,secPart=text:match("^(%d+)[mM](%d+)[sS]$");end if minPart then local minutes=tonumber(minPart) or 0 ;local seconds=tonumber(secPart) or 0 ;return (  
  minutes * 60) + seconds ;end local onlySec=text:match("^(%d+)%s*[sS]$");if (onlySec and  not text:match("[kKmMbB]")) then return tonumber(onlySec) or 0 ;end local numStr 
  ,suf=text:match("([%d%.]+)%s*([kKmMbB]?)");local val=tonumber(numStr) or 0 ;if (suf and (suf~="")) then local lower=suf:lower();if (lower=="k") then val=val * 1000 ;     
  elseif (lower=="m") then val=val * 1000000 ;elseif (lower=="b") then val=val * 1000000000 ;end end return val;end local function formatValue(amount) if (type(amount)~=   
